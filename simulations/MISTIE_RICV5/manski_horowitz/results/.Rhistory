setwd("~/Desktop/manski_horowitz/binary_noRes_5050/results")
rm(list=ls())
lb <- 0
ub <- 0.5
psi <- seq(from = 0, to = 1, by = 0.01)
npsi <- length(psi)
cov_probability <- function(ci, val){
mean(ci[,1] <= val & val <= ci[,2])
}
sampleSizes <- c(200, 500, 1000, 2000)
for (n in sampleSizes){
load(paste("res",n,".Rdata", sep = ""))
print(paste("Average width for", n))
print(mean(ci[,2]-ci[,1]))
cov_results <- rep(0, npsi)
for (i in 1:npsi){
cov_results[i] <- cov_probability(ci, psi[i])
}
cbind(psi, cov_results)
plot(psi, cov_results, type = "l")
abline(v = c(lb, ub))
abline(h = 0.95, col = "red")
abline(h = 0.94, col = "blue")
}
